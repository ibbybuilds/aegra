[project]
name = "$slug"
version = "0.1.0"
description = "$project_name"
requires-python = ">=3.11"

dependencies = [
    "aegra-cli>=$aegra_version",
    "langgraph>=1.0.3",
    "langchain-openai>=1.0.3",
]

[build-system]
requires = ["hatchling"]
build-backend = "hatchling.build"

[tool.hatch.build.targets.wheel]
packages = ["src/$slug"]
